package poly.noInherit;

public class Rabbit {
	private int hp ; // 체력
	private int def; // 방어력
	private int atk; // 공격력
	
	// 초기화 상수
	public static final int INIT_HP  = 3;
	public static final int INIT_ATK = 0;
	public static final int INIT_DEF = 0;
	
	// 생성자 - 변수 초기화
	public Rabbit() {
		hp  = INIT_HP ;
		def = INIT_DEF;
		atk = INIT_ATK;
	}
}
