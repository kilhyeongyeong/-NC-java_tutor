package poly.casting;

public class MainClass1 {

	public static void main(String[] args) {
		// Promotion이 적용되면 자식클래스에만 정의된 요소 조회 불가능
		Parent p = new Child();
		p.method1(); // Parent에만 정의된 메서드
		p.method2(); // 오버라이딩된 메서드는 타입상관업시 인스턴스의 자식쪽 메서드 호출
		p.method3(); // 부모쪽 지역 변수로는 자식쪽에만 있는 요소 호출 불가능
		
		Child c = new Child();
		c.method1();
		c.method2();
		c.method3();
		

	}

}
