package day09;

public class GarbageCollertorTestMain {

	public static void main(String[] args) {
		for(int i=0; i<5; i++) {
			GarbageCollertorTest test = new GarbageCollertorTest(i);
			test = null;
			
			// 쓰레기 수집 강제 실행 명령어(쓸 필요 없음) --전체적으로 할 경우 비효율적... 나눠서 여유가 있을 경우 실행
			System.gc();
		}

	}

}
