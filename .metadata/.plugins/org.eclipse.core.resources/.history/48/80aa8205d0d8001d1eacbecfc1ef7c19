package poly.noInherit;

public class MainClass1 {

	public static void main(String[] args) {
		// 전사 생성
		Warrior wr1 = new Warrior("id**");
		
		// 전사 정보 조회
		wr1.showStatus();
		
		// 토끼 사냥
		wr1.huntRabbit();
		
		// 전사 정보 조회
		wr1.showStatus();
		
		// 쥐 사냥
		wr1.huntRat();
		
		// 전사 정보 조회
		wr1.showStatus();
		
	}

}
